  "bugs": {
    "url": "https://github.com/WebDevWes/Amazaddict/issues"
  },
  "homepage": "https://github.com/WebDevWes/Amazaddict#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.13.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.12"
  }
}
